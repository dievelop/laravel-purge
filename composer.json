{
    "name": "dievelop/laravel-purge",
    "description": "Remove expired files from laravel filesystem disks directories",
    "keywords": [
        "dievelop",
        "laravel",
        "laravel-purge"
    ],
    "require": {
        "php" : "^7.1",
        "illuminate/support": "~5.8.0",
        "nesbot/carbon": "^1.0|^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench":"~3.8.0"
    },
    "autoload": {
        "psr-4": {
            "Dievelop\\LaravelPurge\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dievelop\\LaravelPurge\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dievelop\\LaravelPurge\\ServiceProvider"
            ]
        }
    }
}
